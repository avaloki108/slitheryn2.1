{
  "primary_model": "devstral-2:123b-cloud",
  "reasoning_model": "devstral-2:123b-cloud",
  "comprehensive_model": "devstral-2:123b-cloud",
  "ollama_base_url": "http://localhost:11434",
  "timeout": 120,
  "temperature": 0.1,
  "max_tokens": 2000,
  "enable_ai_analysis": true,
  "confidence_threshold": 0.7,
  "analysis_types": {
    "vulnerability_detection": true,
    "attack_scenarios": true,
    "fix_recommendations": true,
    "severity_assessment": true,
    "false_positive_reduction": true
  },
  "enable_multi_agent": true,
  "agent_types": [
    "vulnerability",
    "exploit",
    "fix",
    "economic",
    "governance"
  ],
  "consensus_threshold": 0.7,
  "parallel_analysis": true,
  "max_workers": 4,
  "enable_rag": true,
  "embedding_provider": "voyage",
  "embedding_model": "voyage-3.5",
  "similarity_threshold": 0.7,
  "max_similar_contracts": 3,
  "cache_embeddings": true,
  "cache_path": ".slitheryn/embeddings_cache/embeddings.json"
}